;; State type
(define-state-type st ((x Real) (y Real)))

;; Some state formulas
(define-states sf_01 st 
  (= x 0)
)

(define-states sf_02 st 
  (and (= x 1) sf_01)
)

(define-transition t_01 st
  (implies state.sf_01 next.sf_02)
)

(define-transition t_02 st
  (or (= next.x state.y) t_01)
)

(define-transition-system s_01 st t_01)

(define-transition-system s_02 st (or t_01 t_02))  
